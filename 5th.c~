#include <mega32.h>
#include <alcd.h>
#include <delay.h>

void main(void)
{
 lcd_init(16);
while (1)
      {           
      int i,a;
      for(i=0;i<=16;i++){ 
     
      lcd_clear();
      lcd_gotoxy(i,0);
      
      
      for(a=0;a<=7;i++){
      lcd_putsf("GUNAWAN 5268");
      } 
      
      delay_ms(1000); 
         
      }
      }
} 
